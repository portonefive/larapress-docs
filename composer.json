{
    "name": "portonefive/larapress",
    "description": "The LaraPress framework. A Laravel-based framework for WordPress developers.",
    "keywords": [
        "larapress",
        "framework",
        "laravel",
        "WordPress"
    ],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Jeff Berry",
            "email": "jeff@portonefive.com",
            "homepage": "http://portonefive.com.com"
        }
    ],
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "raveren/kint": "0.9.*",
        "johnpbloch/wordpress": "4.1.*",
        "portonefive/larapress-framework": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "public/content/mu-plugins/{$name}/": ["larapress/framework"],
            "public/content/plugins/{$name}/": ["type:wordpress-plugin"],
            "public/content/themes/{$name}/": ["larapress/theme"],
            "public/content/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "public/cms"
    }
}
